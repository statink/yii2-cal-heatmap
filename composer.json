{
    "name": "statink/yii2-cal-heatmap",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "cal-heatmap asset bundle for stat.ink",
    "minimum-stability": "stable",
    "require": {
        "php": ">= 8.0",
        "npm-asset/cal-heatmap": "^4.0.0",
        "yiisoft/yii2": "2.0.*"
    },
    "require-dev": {
        "jp3cki/coding-standard": "^0.1.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "fxp-asset": {
            "enabled": false
        },
        "platform": {
            "php": "8.0.28"
        },
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "statink\\yii2\\calHeatmap\\": "src"
        }
    },
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@bouhime.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
